{
    "name": "nyholm/class-requirement-extractor",
    "description": "Extract requirements from classes. Use type hints and Symfony Validation",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nyholm\\ClassRequirementExtractor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nyholm\\ClassRequirementExtractor\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Nyholm",
            "email": "tobias.nyholm@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/property-info": "^6.2 || ^7.0",
        "phpdocumentor/reflection-docblock": "^5.3"
    },
    "require-dev": {
        "symfony/validator": "^6.2 || ^7.",
        "phpunit/phpunit": "^10.4"
    }
}
